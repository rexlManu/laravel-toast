{
    "name": "rexlmanu/laravel-toast",
    "description": "Simple toast messages for Laravel.",
    "homepage": "https://github.com/rexlManu/laravel-toast",
    "license": "MIT",
    "authors": [{
            "name": "Grimthorr",
            "email": "phil@hughesbox.co.uk",
            "homepage": "http://hughesbox.co.uk"
        },
        {
            "name": "Emmanuel Lampe",
            "email": "mail@emmanuel-lampe.de",
            "homepage": "https://emmanuel-lampe.de"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": ">=5.*",
        "illuminate/config": ">=5.*",
        "illuminate/database": ">=5.*"
    },
    "autoload": {
        "psr-4": {
            "Grimthorr\\LaravelToast\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Grimthorr\\LaravelToast\\ServiceProvider"
            ],
            "aliases": {
                "Toast": "Grimthorr\\LaravelToast\\Facade"
            }
        }
    }
}